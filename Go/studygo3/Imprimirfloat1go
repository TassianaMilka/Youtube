
package main //Ponto de entrada  

import "fmt" //format(formatação para imprimir o println)


func main() { //Função prinicipal do programa

    //Declaração de variáveis
    
    var altura float32=1.80;
    var valor float64=400;

    //Imprimir a mensagem no console(print) da altura e valor
    
    fmt.Println("Altura e o valor  é=",altura,valor);


}
